{
  "name": "Flash",
  "version": "1.0.0",
  "description": "Extremely minimal ES2015 Boilerplate",
  "scripts": {
    "test": "tape -r reify buble/register test/*.test.js | tap-spec",
    "test:watch": "tape-watch -r reify buble/register test/*.test.js | tap-spec",
    "watch": "rollup -w -c rollup.config.js",
    "build": "NODE_ENV=production rollup -c rollup.config.js"
  },
  "author": "Theo Lampert",
  "devDependencies": {
    "reify": "^0.6.8",
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-hash": "^1.2.0",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "uglify-js": "^2.8.18"
  }
}
